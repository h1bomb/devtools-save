{
  "name": "devtools-save",
  "description": "Saves files modified in Chrome DevTools",
  "version": "0.7",
  "devtools_page": "devtools.html",
  "options_page": "options.html",
  "background_page": "background.html",
  "permissions": [ 
    "experimental",
    "notifications",
    "http://*/*",
    "https://*/*"
  ],
  "plugins": [
    { "path": "devtools-save.dll" },
    { "path": "devtools-save.plugin" },
    { "path": "libdevtools-save.so" }
  ]
}
